<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deligo.app">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DeliGo">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.deligo.app.ui.auth.LoginActivity" />
        <activity android:name="com.deligo.app.ui.auth.RegisterActivity" />
        <activity android:name="com.deligo.app.ui.customer.CustomerMainActivity" />
        <activity android:name="com.deligo.app.ui.owner.OwnerMainActivity" />
        <activity android:name="com.deligo.app.ui.shipper.ShipperMainActivity" />
        <activity android:name="com.deligo.app.ui.shipper.DeliveryDetailActivity"
            android:parentActivityName="com.deligo.app.ui.shipper.ShipperMainActivity" />
    </application>

</manifest>
